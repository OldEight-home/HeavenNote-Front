.search{
  border-radius: 5px;
  border: 1.5px solid rgb(121, 121, 121);
  color: black;
  height: 25px;
  width: 95%;
  margin-left: 5px;
  font-size: 15px;
}
.line{
  margin-top: 8px;
  height: 2px;
  width:100%;
  background-color: rgb(121, 121, 121);
}
.timer{
  margin-left: 5px;
  margin-top: 3px;
}
.submit{
  margin-left: 130px;
  margin-top: 100%;
  width: 64px;
}

.line1{
  height: 2px;
  width: 100%;
  background-color: rgb(121, 121, 121);
}
.line2{
  height: 1px;
  width: 100%;
  background-color: rgb(121, 121, 121);
}

.content {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.noteInfo {
  position: relative;
  width: 100%;
  height:200rpx;
  box-sizing: border-box;
  padding-left: 10px;
  padding-right: 10px;
  box-shadow: 0 4px 6px rgb(0 0 0 / 6%);
  box-sizing: border-box;
  border-radius: 10px;
}
.title{
  display: flex;
  align-items: center;
  color: #131212;
  font-weight: 600;
  font-size: 20px;
}
.lock{
  display: flex;
  top:2%;
  left:90%;
  height:25px;
  width: 25px;
  position: absolute;
}
.desc{
  display: flex;
  align-items: center;
  font-size: 14px;
}
.bottomInfo{
  flex-direction: row;
}

.dateTime{
  display: flex;
  position: absolute;
  align-items: center;
  bottom: 2%;
  font-size: 10px;
  color: #464343;
}
.noteCount{
  display: flex;
  position: absolute;
  align-items: center;
  bottom: 2%;
  left:86%;
  font-size: 12px;
  color: #464343;
}
.noteCountImg{
  display: flex;
  position: absolute;
  align-items: center;
  height:20px;
  width: 20px;
  bottom:2%;
  left:80%;
}


